{
  "sections": {
    "main": {
      "type": "main-product",
      "blocks": {
        "vendor": {
          "type": "text",
          "settings": {
            "text": "{{ product.vendor }}",
            "text_style": "uppercase"
          }
        },
        "title": {
          "type": "title",
          "settings": {
          }
        },
        "caption": {
          "type": "text",
          "settings": {
            "text": "{{ product.metafields.descriptors.subtitle.value }}",
            "text_style": "subtitle"
          }
        },
        "price": {
          "type": "price",
          "settings": {
          }
        },
        "e28ee9e2-802e-4374-ab01-5f25ff68fe58": {
          "type": "buy_buttons",
          "settings": {
            "show_dynamic_checkout": true,
            "show_gift_card_recipient": true
          }
        }
      },
      "block_order": [
        "vendor",
        "title",
        "caption",
        "price",
        "e28ee9e2-802e-4374-ab01-5f25ff68fe58"
      ],
      "settings": {
        "enable_sticky_info": true,
        "color_scheme": "",
        "media_size": "large",
        "constrain_to_viewport": true,
        "media_fit": "contain",
        "gallery_layout": "stacked",
        "media_position": "left",
        "image_zoom": "lightbox",
        "mobile_thumbnails": "hide",
        "hide_variants": true,
        "enable_video_looping": false,
        "padding_top": 36,
        "padding_bottom": 12
      }
    },
    "3a426c02-cc93-4809-98b3-02f8231f1dd6": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "<div class=\"page-width\">{{product.description}}<\/div>",
        "color_scheme": "",
        "padding_top": 40,
        "padding_bottom": 52
      }
    },
    "custom_liquid_7iVMLx": {
      "type": "custom-liquid",
      "settings": {
        "custom_liquid": "{% for field in product.metafields %}\n test\n{% endfor %}",
        "color_scheme": "",
        "padding_top": 40,
        "padding_bottom": 52
      }
    },
    "related-products": {
      "type": "related-products",
      "settings": {
        "heading": "You may also like",
        "heading_size": "h2",
        "products_to_show": 4,
        "columns_desktop": 4,
        "color_scheme": "scheme-1",
        "image_ratio": "square",
        "image_shape": "default",
        "show_secondary_image": true,
        "show_vendor": false,
        "show_rating": false,
        "columns_mobile": "2",
        "padding_top": 36,
        "padding_bottom": 28
      }
    }
  },
  "order": [
    "main",
    "3a426c02-cc93-4809-98b3-02f8231f1dd6",
    "custom_liquid_7iVMLx",
    "related-products"
  ]
}
